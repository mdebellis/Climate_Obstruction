                                                                                           <https://www.michaeldebellis.com/climate_obstruction/case_categories> "Suits against corporations, individuals, Protesters" ;
                                                                                           <https://www.michaeldebellis.com/climate_obstruction/case_documents> "https://climatecasechart.com/wp-content/uploads/non-us-case-documents/2021/20210514_75402_decision.pdf, https://climatecasechart.com/wp-content/uploads/non-us-case-documents/2022/20221118_75402_decision.pdf, https://climatecasechart.com/wp-content/uploads/non-us-case-documents/2022/20221118_75402_decision-3.pdf, https://climatecasechart.com/wp-content/uploads/non-us-case-documents/2022/20221118_75402_decision-1.pdf" ;
